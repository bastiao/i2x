<h4 class="subheader">Events</h4>
    <ul class="no-bullet">
      <% @events.each do |event| %>
        <li><%= link_to "<i class=\"icon-chevron-right\"></i>#{event.agent.title}".html_safe, event.agent %> <span class="tiny label secondary radius"><%= time_ago_in_words(event.created_at) %> ago</span></li>
      <% end %>
    </ul>